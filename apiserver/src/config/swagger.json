{
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UUID": {
				"type": "string",
				"format": "uuid",
				"description": "Stringified UUIDv4.\nSee [RFC 4112](https://tools.ietf.org/html/rfc4122)",
				"pattern": "[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}"
			},
			"ListIdDTO": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UUID"
					}
				},
				"type": "object",
				"additionalProperties": false
			},
			"CreateListDTO": {
				"properties": {
					"title": {
						"type": "string"
					},
					"closureDate": {
						"type": "string",
						"format": "date-time"
					},
					"ownersIds": {
						"items": {
							"$ref": "#/components/schemas/UUID"
						},
						"type": "array"
					},
					"isShared": {
						"type": "boolean",
						"default": false
					},
					"grantedUsersIds": {
						"items": {
							"$ref": "#/components/schemas/UUID"
						},
						"type": "array"
					}
				},
				"required": [
					"title",
					"ownersIds"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Partial_ListDTO_": {
				"properties": {
					"sharingCode": {
						"$ref": "#/components/schemas/UUID"
					},
					"title": {
						"type": "string"
					},
					"closureDate": {
						"type": "string",
						"format": "date-time"
					},
					"isShared": {
						"type": "boolean",
						"default": false
					}
				},
				"type": "object",
				"description": "Make all properties in T optional"
			},
			"ListDTO": {
				"properties": {
					"sharingCode": {
						"$ref": "#/components/schemas/UUID"
					},
					"title": {
						"type": "string"
					},
					"closureDate": {
						"type": "string",
						"format": "date-time"
					},
					"isShared": {
						"type": "boolean",
						"default": false
					}
				},
				"required": [
					"title"
				],
				"type": "object",
				"additionalProperties": false
			},
			"SelectKindList": {
				"description": "List Selection Kind",
				"enum": [
					"all",
					"owned",
					"granted"
				],
				"type": "string"
			},
			"UserIdDTO": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UUID"
					}
				},
				"type": "object",
				"additionalProperties": false
			},
			"MailAlreadyUsedError": {
				"properties": {
					"name": {
						"type": "string"
					},
					"message": {
						"type": "string"
					},
					"stack": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"message"
				],
				"type": "object",
				"additionalProperties": false
			},
			"email": {
				"type": "string",
				"format": "email",
				"description": "Stringified email.\nSee [Email Regex](https://emailregex.com/) and [RFC 5322](https://tools.ietf.org/html/rfc5322)",
				"pattern": "(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))"
			},
			"CreateUserDTO": {
				"properties": {
					"email": {
						"$ref": "#/components/schemas/email"
					},
					"displayName": {
						"type": "string"
					}
				},
				"required": [
					"email",
					"displayName"
				],
				"type": "object",
				"additionalProperties": false
			},
			"Partial_UserDTO_": {
				"properties": {
					"email": {
						"$ref": "#/components/schemas/email"
					},
					"displayName": {
						"type": "string"
					}
				},
				"type": "object",
				"description": "Make all properties in T optional"
			},
			"UserDTO": {
				"properties": {
					"email": {
						"$ref": "#/components/schemas/email"
					},
					"displayName": {
						"type": "string"
					}
				},
				"required": [
					"email",
					"displayName"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {
			"bearerAuth": {
				"type": "http",
				"scheme": "bearer",
				"bearerFormat": "JWT"
			}
		}
	},
